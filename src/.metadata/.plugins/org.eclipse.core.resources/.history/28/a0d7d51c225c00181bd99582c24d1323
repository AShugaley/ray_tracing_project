	{
		image = new float[500][500];
	}
	
	public RayTracer(int width)
	{
		image = new float[width][width];
	}
	
	public void shootRays()
	{
		for(int i=0; i<image.length; i++)
		{
			for(int j=0; j<image[0].length; j++)
			{
				
			}
		}
	}
}
